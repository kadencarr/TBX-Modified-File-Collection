<?xml version='1.0' encoding="UTF-8"?>
<TBX dialect="TBX-Min">
	<header>
		<id>D001</id>
		<description>A TBX-Min example containing two concept entries with multiple terms in two languages (English and Spanish)</description>
		<languages source="en" target="de"/>
		<creator>BYU TRG</creator>
		<directionality>bidirectional</directionality>
		<license>CC BY license can be freely copied and modified</license>
	</header>
	<body>
		<termEntry id="C001">
			<subjectField>Currency</subjectField>
			<langSet xml:lang="en">
				<ntig>
					<term>money</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>preferred</termStatus>
					<noteGrp><note><noteValue>may refer to physical or banked currency</noteValue></note></noteGrp>
					<customer>TRG</customer>
				</ntig>
				<ntig>
					<term>cash</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>admitted</termStatus>
					<noteGrp><note><noteValue>refers to physical currency</noteValue></note></noteGrp>
					<customer>TRG</customer>
				</ntig>
				<ntig>
					<term>dough</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>admitted</termStatus>
					<noteGrp><note><noteValue>a more recent slang synonym</noteValue></note></noteGrp>
					<customer>TRG</customer>
				</ntig>
				<ntig>
					<term>bread</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>notRecommended</termStatus>
					<noteGrp><note><noteValue>a older slang synonym</noteValue></note></noteGrp>
					<customer>TRG</customer>
				</ntig>
			</langSet>
			<langSet xml:lang="de">
				<ntig>
					<term>Geld</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>preferred</termStatus>
					<noteGrp><note><noteValue>may refer to physical or banked currency</noteValue></note></noteGrp>
					<customer>TRG</customer>
				</ntig>
				<ntig>
					<term>Bargeld</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>admitted</termStatus>
					<noteGrp><note><noteValue>refers to physical currency</noteValue></note></noteGrp>
					<customer>TRG</customer>
				</ntig>
				<ntig>
					<term>Knete</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>admitted</termStatus>
					<noteGrp><note><noteValue>a slang term for Geld, has no plural</noteValue></note></noteGrp>
					<customer>TRG</customer>
				</ntig>
				<ntig>
					<term>Kohle</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>notRecommended</termStatus>
					<noteGrp><note><noteValue>an older slang form used shortly after WW2</noteValue></note></noteGrp>
					<customer>TRG</customer>
				</ntig>
			</langSet>
		</termEntry>
		<termEntry id="C002">
			<subjectField>Music</subjectField>
			<langSet xml:lang="en">
				<ntig>
					<term>music</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>preferred</termStatus>
					<noteGrp><note><noteValue>may mean music in general or may mean an instance of music ("Let's play music")</noteValue></note></noteGrp>
					<customer>TRG</customer>
				</ntig>
				<ntig>
					<term>jam</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>notRecommended</termStatus>
					<noteGrp><note><noteValue>slang for music, specifically the making of, used mostly by musicians (i.e. "Let's have a jam!") </noteValue></note></noteGrp>
					<customer>TRG</customer>
				</ntig>
			</langSet>
			<langSet xml:lang="de">
				<ntig>
					<term>Musik</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>preferred</termStatus>
					<customer>TRG</customer>
				</ntig>
				<ntig>
					<term>Mucke</term>
					<partOfSpeech>noun</partOfSpeech>
					<termStatus>notRecommended</termStatus>
					<noteGrp><note><noteValue>slang term for music, specifically the making of, used mostly by musicians (i.e. "Haben wir 'ne Mucke!")</noteValue></note></noteGrp>
					<customer>TRG</customer>
				</ntig>
			</langSet>
		</termEntry>
	</body>
</TBX>			